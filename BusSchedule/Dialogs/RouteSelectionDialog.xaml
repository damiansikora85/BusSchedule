<?xml version="1.0" encoding="utf-8" ?>
<pages:PopupPage  xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:pages="clr-namespace:Rg.Plugins.Popup.Pages;assembly=Rg.Plugins.Popup"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BusSchedule.Dialogs.RouteSelectionDialog">
    <Frame Margin="12">
        <StackLayout>
            <Button Text="{Binding FirstRouteName}" Clicked="FirstRouteClicked"/>
            <Button Text="{Binding SecondRouteName}" Clicked="SecondRouteClicked"/>
        </StackLayout>
    </Frame>
</pages:PopupPage>