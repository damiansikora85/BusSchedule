<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BusSchedule.Pages.ThreadsTestPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding ThreadsNum}"/>
            <Label Text="{Binding ThreadsMin}"/>
            <Label Text="{Binding ThreadsMax}"/>
            <Button Text="Schedule Task" Clicked="Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>