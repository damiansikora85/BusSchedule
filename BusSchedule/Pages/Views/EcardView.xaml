<?xml version="1.0" encoding="UTF-8" ?>
<ContentView
    x:Class="BusSchedule.Pages.Views.EcardView"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentView.Content>
        <StackLayout>
            <Entry Keyboard="Numeric" Text="{Binding CardNumber}"/>
            <Button Text="Szukaj" Command="{Binding FindCardCommand}"/>
        </StackLayout>
    </ContentView.Content>
</ContentView>